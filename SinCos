import numpy as np                  #import numpy
import matplotlib.pyplot as plt     #import matplotlib
x=np.linspace(0,6,50)               #create x
xlist = np.sin(x)                   
ylist = np.tan(x)
plt.plot(xlist) 
plt.plot(ylist)
plt.title('Cos and Sin')            #Add title
plt.show()                          #Show plot
